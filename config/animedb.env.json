{
    "log" : {
        "level" : "trace"
    },
    "archive" : {
        "enable" : true,
        "unique" : false
    },
    "database" : {
        "path" : "database/template.sqlite3"
    },
    "myanimelist" : {
        "auth" : "auth/myanimelist.json",
        "regex" : {
            "searchQuery" : "[^\\w\\s-]",
            "themeParseDefault" : "^((#)?)(([0-9]+)?)((:)?)(([ \"]+)?)((.)*?)((\\((.+)\\))?)((\"+)?)( (b|B)y )((.)*?)((\\((.+)\\))?)$",
            "themeParseNoMatch" : "^((#)?)(([0-9]+)?)((:)?)(([ \"]+)?)((.)*?)((\\((.+)\\))?)(\" )((.)*?)((\\((.+)\\))?)$"
        }
    },
    "youtube" : {
        "autoReAuth" : true,
        "auth" : [
            "auth/robot01.json"
        ]
    },
    "command" : {
        "seasons" : {
            "delay" : 200,
            "list" : [
                { "season" : "winter", "year" : 2021 },
                { "season" : "spring", "year" : 2021 },
                { "season" : "summer", "year" : 2021 },
                { "season" : "fall", "year" : 2021 },
                { "season" : "winter", "year" : 2022 },
                { "season" : "spring", "year" : 2022 },
                { "season" : "summer", "year" : 2022 },
                { "season" : "fall", "year" : 2022 }
            ]
        },
        "personal" : {
            "delay" : 1000,
            "userName" : "username"
        },
        "scout" : {
            "delay" : 1000,
            "queryLengthLimit" : 40,
            "startDateOffsetLimit" : 1,
            "base" : "SELECT * FROM v_AniListToScout",
            "criteria" : "WHERE UserName = 'username' ORDER BY AniListId ASC",
            "limit" : "LIMIT 5"
        },
        "themes" : {
            "delay" : 1000,
            "base" : "SELECT * FROM v_ThemesToSearch",
            "criteria" : "WHERE UserName = 'username' ORDER BY AniListId ASC",
            "limit" : "LIMIT 5"
        },
        "medias" : {
            "delay" : 100,
            "base" : "SELECT * FROM v_MediasToSearch",
            "criteria" : "ORDER BY AniListId",
            "limit" : "LIMIT 5"
        },
        "animepick" : {
            "delay" : 200,
            "list" : [
                { "aniListId" : 1, "myAnimeListId" : 1 }
            ]
        },
        "mediapick" : {
            "delay" : 200,
            "list" : [
                { "keyId" : "n2rVnRwW0h8", "themeId" : 1 },
                { "keyId" : "_Eb5LMjDYEw", "themeId" : 2 }
            ]
        }
    }
}