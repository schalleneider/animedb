{
  "name": "animedb",
  "version": "1.0.0",
  "description": "animedb",
  "type": "module",
  "exports": "./main.js",
  "scripts": {
    "help": "node main.js --help",
    "start": "node main.js",
    "start:seasons:anilist": "node main.js seasons --source='anilist'",
    "start:seasons:myanimelist": "node main.js seasons --source='myanimelist'",
    "start:personal:anilist": "node main.js personal --source='anilist'",
    "start:scout:myanimelist": "node main.js scout --source='myanimelist'",
    "start:themes:myanimelist": "node main.js themes --source='myanimelist'",
    "start:medias:youtube": "node main.js medias --source='youtube'",
    "redo:seasons:anilist": "node main.js seasons --source='anilist' --archive --archivePath='archive/anilist_seasons.json' ",
    "redo:seasons:myanimelist": "node main.js seasons --source='myanimelist' --archive --archivePath='archive/myanimelist_seasons.json'",
    "redo:personal:anilist": "node main.js personal --source='anilist' --archive --archivePath='archive/anilist_personal.json'",
    "redo:scout:myanimelist": "node main.js scout --source='myanimelist' --archive --archivePath='archive/myanimelist_scout.json'",
    "redo:themes:myanimelist": "node main.js themes --source='myanimelist' --archive --archivePath='archive/myanimelist_themes.json'",
    "redo:medias:youtube": "node main.js medias --source='youtube' --archive --archivePath='archive/youtube_medias.json'"
  },
  "author": "wschneider",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.26.1",
    "axios-retry": "^3.2.4",
    "chalk": "^5.0.1",
    "eslint": "^8.12.0",
    "googleapis": "^100.0.0",
    "log4js": "^6.4.4",
    "moment": "^2.29.1",
    "prompt": "^1.2.2",
    "sqlite": "^4.0.25",
    "sqlite3": "^5.0.2",
    "yargs": "^17.4.0"
  }
}
