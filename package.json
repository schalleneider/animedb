{
  "name": "animedb",
  "version": "1.0.0",
  "description": "animedb",
  "type": "module",
  "exports": "./main.js",
  "scripts": {
    "redist": "npx caxa --input '.' --output './bin/animedb.exe' -- '{{caxa}}/node_modules/.bin/node' '{{caxa}}/main.js'",
    "dev:help": "node main.js --help",
    "dev:start:seasons:anilist": "node main.js seasons --source='anilist'",
    "dev:start:seasons:myanimelist": "node main.js seasons --source='myanimelist'",
    "dev:start:personal:anilist": "node main.js personal --source='anilist'",
    "dev:start:scout:myanimelist": "node main.js scout --source='myanimelist'",
    "dev:start:themes:myanimelist": "node main.js themes --source='myanimelist'",
    "dev:start:medias:youtube": "node main.js medias --source='youtube'",
    "dev:redo:seasons:anilist": "node main.js seasons --source='anilist' --archive --archivePath='archive/anilist_seasons.json' ",
    "dev:redo:seasons:myanimelist": "node main.js seasons --source='myanimelist' --archive --archivePath='archive/myanimelist_seasons.json'",
    "dev:redo:personal:anilist:sakurahanafubuki": "node main.js personal --env=live --source='anilist' --archive --archivePath='archive/anilist_personal_saurahanafubuki.json'",
    "dev:redo:personal:anilist:wschneider": "node main.js personal --env=live --source='anilist' --archive --archivePath='archive/anilist_personal_wschneider.json'",
    "dev:redo:scout:myanimelist": "node main.js scout --source='myanimelist' --archive --archivePath='archive/myanimelist_scout.json'",
    "dev:redo:themes:myanimelist": "node main.js themes --source='myanimelist' --archive --archivePath='archive/myanimelist_themes.json'",
    "dev:redo:medias:youtube": "node main.js medias --source='youtube' --archive --archivePath='archive/youtube_medias.json'",
    "help": "./animedb.exe --help",
    "start:seasons:anilist": "./animedb.exe seasons --env=live --source='anilist'",
    "start:seasons:myanimelist": "./animedb.exe seasons --env=live --source='myanimelist'",
    "start:personal:anilist": "./animedb.exe personal --env=live --source='anilist'",
    "start:scout:myanimelist": "./animedb.exe scout --env=live --source='myanimelist'",
    "start:themes:myanimelist": "./animedb.exe themes --env=live --source='myanimelist'",
    "start:medias:youtube": "./animedb.exe medias --env=live --source='youtube'",
    "redo:seasons:anilist": "./animedb.exe seasons --env=live --source='anilist' --archive --archivePath='archive/anilist_seasons.json'",
    "redo:seasons:myanimelist": "./animedb.exe seasons --env=live --source='myanimelist' --archive --archivePath='archive/myanimelist_seasons.json'",
    "redo:personal:anilist:sakurahanafubuki": "./animedb.exe personal --env=live --source='anilist' --archive --archivePath='archive/anilist_personal_saurahanafubuki.json'",
    "redo:personal:anilist:wschneider": "./animedb.exe personal --env=live --source='anilist' --archive --archivePath='archive/anilist_personal_wschneider.json'",
    "redo:scout:myanimelist": "./animedb.exe scout --env=live --source='myanimelist' --archive --archivePath='archive/myanimelist_scout.json'",
    "redo:themes:myanimelist": "./animedb.exe themes --env=live --source='myanimelist' --archive --archivePath='archive/myanimelist_themes.json'",
    "redo:medias:youtube": "./animedb.exe medias --env=live --source='youtube' --archive --archivePath='archive/youtube_medias.json'"
  },
  "author": "wschneider",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.26.1",
    "axios-retry": "^3.2.4",
    "caxa": "^2.1.0",
    "chalk": "^5.0.1",
    "eslint": "^8.12.0",
    "googleapis": "^100.0.0",
    "log4js": "^6.4.4",
    "moment": "^2.29.1",
    "prompt": "^1.2.2",
    "sqlite": "^4.0.25",
    "sqlite3": "^5.0.2",
    "yargs": "^17.4.0"
  }
}
