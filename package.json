{
  "name": "animedb",
  "version": "1.0.0",
  "description": "animedb",
  "type": "module",
  "exports": "./main.js",
  "scripts": {
    "start": "node main.js",
    "start:seasons:anilist": "node main.js seasons --database='database/template.sqlite3' --source='anilist' --config='config/seasons.json'",
    "start:seasons:myanimelist": "node main.js seasons --database='database/template.sqlite3' --source='myanimelist' --config='config/seasons.json'",
    "start:personal:anilist": "node main.js personal --database='database/template.sqlite3' --source='anilist' --config='config/personal.json'",
    "start:scout:myanimelist": "node main.js scout --database='database/template.sqlite3' --source='myanimelist' --config='config/scout.json'",
    "start:themes:myanimelist": "node main.js themes --database='database/template.sqlite3' --source='myanimelist' --config='config/themes.json'",
    "redo:seasons:anilist": "node main.js seasons --database='database/template.sqlite3' --source='anilist' --config='archive/anilist_seasons.json' --archive",
    "redo:seasons:myanimelist": "node main.js seasons --database='database/template.sqlite3' --source='myanimelist' --config='archive/myanimelist_seasons.json' --archive",
    "redo:personal:anilist": "node main.js personal --database='database/template.sqlite3' --source='anilist' --config='archive/anilist_personal.json' --archive",
    "redo:scout:myanimelist": "node main.js scout --database='database/template.sqlite3' --source='myanimelist' --config='archive/myanimelist_scout.json' --archive",
    "redo:themes:myanimelist": "node main.js themes --database='database/template.sqlite3' --source='myanimelist' --config='archive/myanimelist_themes.json' --archive"
  },
  "author": "wschneider",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.26.1",
    "axios-retry": "^3.2.4",
    "chalk": "^5.0.1",
    "eslint": "^8.12.0",
    "log4js": "^6.4.4",
    "moment": "^2.29.1",
    "prompt": "^1.2.2",
    "sqlite": "^4.0.25",
    "sqlite3": "^5.0.2",
    "yargs": "^17.4.0"
  }
}
